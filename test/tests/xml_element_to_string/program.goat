import "xml.goat";

var elem = new XML.Element();
elem.tag = "root";
elem.content = [];

var child = new XML.Element();
child.tag = "empty";
child.attrib = {};
child.attrib.first = 1;
child.attrib.second = "2nd";
elem.content.push(child);

elem.content.push("Text element");

child = new XML.Element();
child.tag = "non_empty";
child.attrib = {};
child.attrib.symbol = '$';
child.content = "data";
elem.content.push(child);

print(elem.toString());
